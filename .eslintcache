obj) => {\r\n        let newFile = document.createElement(\"div\");\r\n        let ime = document.getElementById(\"fileList\");\r\n\r\n        ReactDOM.render(\r\n            <input id={\"file\" + (++window.fileCount)} type=\"file\" className=\"file\" onChange={AddFileElement}/>\r\n        , newFile)\r\n\r\n        ime.appendChild(newFile);\r\n\r\n        if(obj.target.files) {\r\n            let inputFile = obj.target.files[0];\r\n\r\n            let reader = new FileReader();\r\n\r\n            reader.onload = (e) => {\r\n                let imgArea = document.getElementsByClassName(\"imgList\")[0];\r\n\r\n                let newImgArea = document.createElement(\"div\");\r\n                imgArea.appendChild(newImgArea);\r\n\r\n                ReactDOM.render(\r\n                    <ImgItem src={e.target.result}/>\r\n                , newImgArea);\r\n            }\r\n\r\n            reader.readAsDataURL(inputFile);\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <Nav/>\r\n            <div className=\"ContentMainWrite\">\r\n\r\n                <div className=\"titleInputDiv\">\r\n                    <input id=\"title\" type=\"text\" placeholder=\"제목\"/>\r\n                </div>\r\n\r\n                <div className=\"usingTechDiv\">\r\n                    <div className=\"utHeader\">\r\n                        <div>사용 기술</div>\r\n                        <div className=\"addButton\" onClick={AddTech}>\r\n                            <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"18.65\" height=\"18.65\" viewBox=\"0 0 18.65 18.65\">\r\n                                <path id=\"Icon_awesome-plus-circle\" data-name=\"Icon awesome-plus-circle\" d=\"M9.887.563a9.325,9.325,0,1,0,9.325,9.325A9.323,9.323,0,0,0,9.887.563ZM15.3,10.94a.453.453,0,0,1-.451.451H11.391v3.459a.453.453,0,0,1-.451.451H8.835a.453.453,0,0,1-.451-.451V11.391H4.924a.453.453,0,0,1-.451-.451V8.835a.453.453,0,0,1,.451-.451H8.383V4.924a.453.453,0,0,1,.451-.451H10.94a.453.453,0